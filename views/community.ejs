<script>
    var data_list = null;

    start()

    function start() {

        $.ajax({
            url: "/community_insert",
            dataType: "json",
            type: "get",
            success: function (data) {
                data_list = data
                console.log(data);
                var html = ''
                for (var i = 0; i < data.length; i++) {
                    var pk_id = data[i].pk_id
                    var date = data[i].date
                    var title = data[i].title
                    var categories = data[i].categories
                    html += '<tr>'
                    html += '<td id="pk_id" name="pk_id">' + pk_id + '</td>'
                    html += '<td id="date" name="date">' + date + '</td>'
                    html += '<td id="title" name="title">' + title + '</td>'
                    html += '<td id="categories" name="categories">' + categories + '</td>'
                    html += '<button class="primary-btn text-uppercase mt-15" name="pk_id"onclick="link_to_detail(' + pk_id + ')">continue Reading</button>'
                    html += '<td><button class="btn btn-primary" data-toggle="modal" data-target="#edit_information" onclick="updata_sql2(' + pk_id + ');">자세하게보러가기</button></td>'
                    html += '</tr>'
                    $("#table_list").html(html);
                }
            },

            error: function (data) {
                console.log(data);
            },
        });
    }
    function updata_sql2(pk_id) {
        var pk_id = pk_id

        localStorage.setItem("#pk_id", pk_id);//pk 가져옴
        location.href = "community_details"
    }
</script>

